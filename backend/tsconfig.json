{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": ".",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": [
      "node",
      "jest"
    ]
  },
  "include": [
    "src",
    "generated"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
